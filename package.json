{
  "name": "ng-util",
  "version": "10.1.0-rc.2",
  "description": "Angular utilities",
  "keywords": [
    "angular",
    "util",
    "Angular utilities"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-util/ng-util/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-util/ng-util.git"
  },
  "scripts": {
    "postinstall": "scripts/ci/install.sh",
    "lint": "tslint -p tsconfig.json -c tslint.json 'packages/**/*.ts'",
    "test": "ng test --no-progress --browsers=ChromeHeadlessCI --code-coverage --no-watch",
    "site:build": "ng b --prod",
    "build": "bash scripts/ci/build.sh",
    "build_artifacts": "bash scripts/ci/build-artifacts.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "pre-publish": "npm run build && node scripts/publish/publish.js",
    "publish:next": "bash scripts/publish/publish.sh -next",
    "publish": "bash scripts/publish/publish.sh"
  },
  "dependencies": {
    "@angular/animations": "~10.0.0",
    "@angular/common": "~10.0.0",
    "@angular/compiler": "~10.0.0",
    "@angular/core": "~10.0.0",
    "@angular/forms": "~10.0.0",
    "@angular/platform-browser": "~10.0.0",
    "@angular/platform-browser-dynamic": "~10.0.0",
    "@angular/router": "~10.0.0",
    "rxjs": "~6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3",
    "@stackblitz/sdk": "^1.3.0",
    "codesandbox": "^2.1.14",
    "monaco-editor": "^0.20.0",
    "vditor": "^3.3.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.0",
    "@angular/cli": "~10.0.0",
    "@angular/compiler-cli": "~10.0.0",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0-next.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5",
    "tslint-config-prettier": "^1.18.0",
    "prettier": "^2.0.5",
    "prettier-stylelint": "^0.4.2",
    "codecov": "^3.5.0",
    "ng-packagr": "^10.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-spec-reporter": "0.0.32",
    "karma-viewport": "^1.0.6",
    "@types/fs-extra": "^9.0.1",
    "karma-summary-reporter": "^1.7.2",
    "karma-parallel": "^0.3.1",
    "conventional-changelog-cli": "^2.0.31",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.3",
    "klaw-sync": "^6.0.0",
    "tsickle": "^0.38.1",
    "stylelint": "^13.3.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.3.0",
    "stylelint-order": "^4.0.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "lint-staged": "^10.1.2",
    "readline-sync": "^1.4.10",
    "pretty-quick": "^2.0.1",
    "@commitlint/cli": "^9.0.0",
    "@commitlint/config-angular": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
