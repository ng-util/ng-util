{
  "name": "ng-util",
  "version": "11.1.0",
  "description": "Angular utilities",
  "keywords": [
    "angular",
    "util",
    "Angular utilities"
  ],
  "author": "cipchk <cipchk@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-util/ng-util/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-util/ng-util.git"
  },
  "scripts": {
    "postinstall": "scripts/ci/install.sh",
    "lint": "tslint -p tsconfig.json -c tslint.json 'packages/**/*.ts'",
    "test": "ng test --no-progress --browsers=ChromeHeadlessCI --code-coverage --no-watch",
    "site:build": "ng b --prod",
    "build": "bash scripts/ci/build.sh",
    "build_artifacts": "bash scripts/ci/build-artifacts.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "pre-publish": "node scripts/publish/publish.js",
    "publish:next": "bash scripts/publish/publish.sh -next",
    "publish": "bash scripts/publish/publish.sh"
  },
  "dependencies": {
    "@angular/animations": "~11.0.0",
    "@angular/common": "~11.0.0",
    "@angular/compiler": "~11.0.0",
    "@angular/core": "~11.0.0",
    "@angular/forms": "~11.0.0",
    "@angular/platform-browser": "~11.0.0",
    "@angular/platform-browser-dynamic": "~11.0.0",
    "@angular/router": "~11.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2",
    "@stackblitz/sdk": "^1.3.0",
    "codesandbox": "^2.1.14",
    "monaco-editor": "^0.20.0",
    "vditor": "^3.3.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.0",
    "@angular/cli": "~11.0.0",
    "@angular/compiler-cli": "~11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2",
    "ng-packagr": "^11.0.1",
    "tsickle": "^0.39.0",
    "codecov": "^3.8.1",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "tslint-config-prettier": "^1.18.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-angular": "^11.0.0",
    "conventional-changelog-cli": "^2.0.31",
    "husky": "^4.2.3",
    "readline-sync": "^1.4.10",
    "klaw-sync": "^6.0.0",
    "@types/fs-extra": "^9.0.1",
    "fs-extra": "^9.0.1",
    "karma-spec-reporter": "^0.0.32",
    "karma-summary-reporter": "^1.7.2",
    "karma-viewport": "^1.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
